{
    "manifest_version": 2,
    "name": "Domain Specific Dictionary",
    "description": "Save abbreviations",
    "version": "0.2",
    "icons": {
        "16": "icons/search.png"
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage",
        "alarms"
    ],
    "background": {"scripts":["background.js"]}
}