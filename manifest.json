{
    "manifest_version": 3,
    "name": "Domain Specific Dictionary",
    "description": "Save abbreviations",
    "version": "0.2",
    "icons": {
        "16": "icons/search.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "scripting",
        "storage",
        "alarms"
    ],
    "background": {
        "service_worker": "background/background.js",
        "type": "module"
    }
}